<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
        "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean id="mainFrame" class="addressBookPackage.AddressBookView" init-method="init">
        <property name="title">
            <value>Address Book </value>
        </property>
        <constructor-arg ref="AddressbookId"/>


    </bean>
    <bean id="AddressbookId" class="addressBookPackage.AddressBook"/>
</beans>